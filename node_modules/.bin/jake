#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules/jake/bin/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules/jake/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/node_modules:/home/pablo/code/cursos/fullstackOpen/node_modules:/home/pablo/code/cursos/node_modules:/home/pablo/code/node_modules:/home/pablo/node_modules:/home/node_modules:/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules/jake/bin/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules/jake/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/node_modules:/home/pablo/code/cursos/fullstackOpen/node_modules:/home/pablo/code/cursos/node_modules:/home/pablo/code/node_modules:/home/pablo/node_modules:/home/node_modules:/node_modules:/home/pablo/code/cursos/fullstackOpen/part3/simpleWebServer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../jake/bin/cli.js" "$@"
fi
